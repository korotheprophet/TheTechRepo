{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {},
  "routes": [
    {
      "src": "/files/(.*)",
      "dest": "/files/$1"
    }
  ],
  "headers": [
    {
      "source": "/files/(.*)",
      "headers": [
        {
          "key": "Content-Disposition",
          "value": "attachment"
        }
      ]
    }
  ],
  "framework": "vite"
}